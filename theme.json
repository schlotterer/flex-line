{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "840px",
			"wideSize": "1200px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "Extra Small",
					"size": "8px",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "16px",
					"slug": "small"
				},
				{
					"name": "Medium ( 32px / 24px )",
					"size": "clamp(24px, calc(1.5rem + ((1vw - 7.68px) * 1.5625)), 32px)",
					"slug": "medium"
				},
				{
					"name": "Large ( 32px / 48px )",
					"size": "clamp(32px, calc(2rem + ((1vw - 7.68px) * 3.125)), 48px)",
					"slug": "large"
				},
				{
					"name": "Extra Large ( 32px / 64px )",
					"size": "clamp(32px, calc(2rem + ((1vw - 7.68px) * 6.25)), 64px)",
					"slug": "x-large"
				},
				{
					"name": "2xl ( 32px / 80px )",
					"size": "clamp(32px, calc(2rem + ((1vw - 7.68px) * 9.375)), 80px)",
					"slug": "xx-large"
				},
				{
					"name": "3xl ( 48px / 96px )",
					"size": "clamp(48px, calc(3rem + ((1vw - 7.68px) * 9.375)), 96px)",
					"slug": "xxx-large"
				}			
			],
			"units": [
				"px",
				"em",
				"rem",
				"svh",
				"vh",
				"vw",
				"%"
			]
		},
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [
				{
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--contrast) 100%)",
					"name": "Primary to Contrast",
					"slug": "primary-contrast"
				},
				{
					"gradient": "linear-gradient(90deg, var(--wp--custom--color--primary--700) 0%, var(--wp--custom--color--primary--900) 100%)",
					"name": "Primary 700 to 900",
					"slug": "primary-700-900"
				},
				{
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--secondary) 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				},
				{
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--alternate) 100%)",
					"name": "Primary to Alternate",
					"slug": "primary-alternate"
				}
			],
			"palette": [
				{
					"color": "#0F6185",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#39B26A",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#004f94",
					"name": "Alternate",
					"slug": "alternate"
				},
				{
					"color": "#FB8C7B",
					"name": "Highlight",
					"slug": "highlight"
				},
				{
					"color": "#FFDD38",
					"name": "Warning",
					"slug": "warning"
				},
				{
					"color": "#18A957",
					"name": "Success",
					"slug": "success"
				},
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "var(--wp--custom--color--gray--200)",
					"name": "Neutral",
					"slug": "neutral"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "Outfit, sans-serif",
					"name": "Brand",
					"slug": "brand",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Outfit",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [ "file:./assets/fonts/default/outfit/Outfit-Variable.woff2" ]
						}
					]
				},
                {
					"fontFamily": "Outfit, sans-serif",
					"name": "Display",
					"slug": "display",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Outfit",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [ "file:./assets/fonts/default/outfit/Outfit-Variable.woff2" ]
						}
					]
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "Body",
					"slug": "body"
				},
                {
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "xSmall",
					"size": "16px",
					"slug": "x-small"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "18px",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "18px",
						"max": "20px"
					},
					"name": "Medium",
					"size": "20px",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "20px",
						"max": "24px"
					},
					"name": "Large",
					"size": "24px",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "24px",
						"max": "30px"
					},
					"name": "xLarge",
					"size": "30px",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "30px",
						"max": "36px"
					},
					"name": "36px",
					"size": "36px",
					"slug": "max-36"
				},
				{
					"fluid": {
						"min": "36px",
						"max": "48px"
					},
					"name": "48px",
					"size": "48px",
					"slug": "max-48"
				},
				{
					"fluid": {
						"min": "42px",
						"max": "60px"
					},
					"name": "60px",
					"size": "60px",
					"slug": "max-60"
				},
				{
					"fluid": {
						"min": "48px",
						"max": "72px"
					},
					"name": "72px",
					"size": "72px",
					"slug": "max-72"
				}
			]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Light",
					"slug": "light",
					"shadow": "0 0 15px rgb(0 0 0 / 15%);"
				},
				{
					"name": "Dark",
					"slug": "dark",
					"shadow": "0 0 10px rgb(0 0 0 / 25%);"
				},
				{
					"name": "Diffused",
					"slug": "diffused",
					"shadow": "0 0 25px rgb(0 0 0 / 15%);"
				}
			]
		},
		"custom": {		
			"color":{
				"brand":{
					"blue" : "#0F6185",
					"green" : "#39B26A",
					"orange" : "#FB8C7B"
				},
				"primary": {
					"main" : "var(--wp--preset--color--primary)",
					"50": "color-mix(in srgb, var(--wp--preset--color--primary) 25%, #fff 100%)",
					"100": "color-mix(in srgb, var(--wp--preset--color--primary) 30%, #fff 80%)",
					"200": "color-mix(in srgb, var(--wp--preset--color--primary) 40%, #fff 60%)",
					"300": "color-mix(in srgb, var(--wp--preset--color--primary) 60%, #fff 40%)",
					"400": "color-mix(in srgb, var(--wp--preset--color--primary) 80%, #fff 20%)",
					"500": "var(--wp--preset--color--primary)",
					"600": "color-mix(in srgb, var(--wp--preset--color--primary) 80%, #000 20%)",
					"700": "color-mix(in srgb, var(--wp--preset--color--primary) 60%, #000 40%)",
					"800": "color-mix(in srgb, var(--wp--preset--color--primary) 40%, #000 60%)",
					"900": "color-mix(in srgb, var(--wp--preset--color--primary) 30%, #000 80%)",
					"950": "color-mix(in srgb, var(--wp--preset--color--primary) 25%, #000 100%)"
				},
				"secondary": {
					"main": "var(--wp--preset--color--secondary)",
					"50": "color-mix(in srgb, var(--wp--preset--color--secondary) 25%, #fff 100%)",
					"100": "color-mix(in srgb, var(--wp--preset--color--secondary) 30%, #fff 80%)",
					"200": "color-mix(in srgb, var(--wp--preset--color--secondary) 40%, #fff 60%)",
					"300": "color-mix(in srgb, var(--wp--preset--color--secondary) 60%, #fff 4%)",
					"400": "color-mix(in srgb, var(--wp--preset--color--secondary) 80%, #fff 20%)",
					"500": "var(--wp--preset--color--secondary)",
					"600": "color-mix(in srgb, var(--wp--preset--color--secondary) 80%, #000 20%)",
					"700": "color-mix(in srgb, var(--wp--preset--color--secondary) 60%, #000 40%)",
					"800": "color-mix(in srgb, var(--wp--preset--color--secondary) 40%, #000 60%)",
					"900": "color-mix(in srgb, var(--wp--preset--color--secondary) 30%, #000 80%)",
					"950": "color-mix(in srgb, var(--wp--preset--color--secondary) 25%, #000 100%)"
				},
				"gray": {
					"50": "color-mix(in srgb, var(--wp--preset--color--white) 95%, var(--wp--preset--color--black) 5%)",
					"100": "color-mix(in srgb, var(--wp--preset--color--white) 90%, var(--wp--preset--color--black) 10%)",
					"200": "color-mix(in srgb, var(--wp--preset--color--white) 80%, var(--wp--preset--color--black) 20%)",
					"300": "color-mix(in srgb, var(--wp--preset--color--white) 70%, var(--wp--preset--color--black) 30%)",
					"400": "color-mix(in srgb, var(--wp--preset--color--white) 60%, var(--wp--preset--color--black) 40%)",
					"500": "color-mix(in srgb, var(--wp--preset--color--white) 50%, var(--wp--preset--color--black) 50%)",
					"600": "color-mix(in srgb, var(--wp--preset--color--white) 40%, var(--wp--preset--color--black) 60%)",
					"700": "color-mix(in srgb, var(--wp--preset--color--white) 30%, var(--wp--preset--color--black) 70%)",
					"800": "color-mix(in srgb, var(--wp--preset--color--white) 20%, var(--wp--preset--color--black) 80%)",
					"900": "color-mix(in srgb, var(--wp--preset--color--white) 10%, var(--wp--preset--color--black) 90%)",
					"950": "color-mix(in srgb, var(--wp--preset--color--white) 5%, var(--wp--preset--color--black) 95%)"
				},
				"alert": {
					"success": "#18A957",
					"warning": "#FFDD38",
					"error": "#DF1642"
				}
			},
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"body": 1.75,
				"heading": 1.1,
				"medium": 1.5,
				"one": 1
			},
			"spacing": {
				"gap": "30px"
			},
			"textShadow": "0 .2rem .4rem rgba(0,0,0, 0.5)",
			"borderRadius": "5px",
			"buttonBorderRadius": "5px",
			"transitionTime": "0.35s ease-in-out"

		},
		"blocks": {
			"core/button": {
				"spacing": {
					"spacingSizes": [
						{
							"name": "xSmall",
							"size": "2px",
							"slug": "x-small"
						},
						{
							"name": "Small",
							"size": "5px",
							"slug": "small"
						},
						{
							"name": "Medium",
							"size": "10px",
							"slug": "medium"
						},
						{
							"name": "Large",
							"size": "16px",
							"slug": "large"
						},
						{
							"name": "xLarge",
							"size": "24px",
							"slug": "x-large"
						},
						{
							"name": "Mega",
							"size": "36px",
							"slug": "mega"
						}
					]
				}
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--gap)",
			"padding": {
				"top": "0px",
				"right": "var(--wp--custom--spacing--gap)",
				"bottom": "0px",
				"left": "var(--wp--custom--spacing--gap)"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "var(--wp--custom--font-weight--light)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--button-border-radius)"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"textTransform": "uppercase"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--small)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				}
			},
			"caption": {
				"spacing": {
					"margin": {
						"top": "10px",
						"bottom": "0px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"cite": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "inherit"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--max-36)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			}
		},
		"blocks": {
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/button": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--large)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--large)"
					}
				},
				"variations": {
					"outline" : {
						"border": {
							"color": "currentColor",
							"style": "solid",
							"width": "1px"
						},
						"spacing": {
							"padding": {
								"top": "calc( var(--wp--preset--spacing--medium) - 1px )",
								"right": "calc( var(--wp--preset--spacing--large) - 1px )",
								"bottom": "calc( var(--wp--preset--spacing--medium) - 1px )",
								"left": "calc( var(--wp--preset--spacing--large) - 1px )"
							}
						}
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--small)"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--gap)",
						"right": "var(--wp--custom--spacing--gap)",
						"bottom": "var(--wp--custom--spacing--gap)",
						"left": "var(--wp--custom--spacing--gap)"
					}
				}
			},
			"core/group": {
				"css": ""
			},
			"core/image": {
				"css": "",
				"spacing": {
					"margin": {
						"top": "var(--wp--custom--spacing--gap)",
						"right": "0",
						"bottom": "var(--wp--custom--spacing--gap)",
						"left": "0"
					}
				},
				"border": {
					"radius": "var(--wp--custom--border-radius)"
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "var(--wp--custom--spacing--gap)"
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/paragraph": {
				"css": ".has-background {padding: var(--wp--preset--spacing--medium) var(--wp--preset--spacing--large);}"
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "& code {overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--gap)",
						"right": "var(--wp--custom--spacing--gap)",
						"bottom": "var(--wp--custom--spacing--gap)",
						"left": "var(--wp--custom--spacing--gap)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"border": {
					"radius": "var(--wp--custom--border-radius)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--x-large)"
					}
				}
			},
			"core/comments-title": {
				"spacing": {
					"margin": {
						"top": "var(--wp--custom--spacing--gap)",
						"bottom": "var(--wp--custom--spacing--gap)"
					}
				}
			},
			"core/post-comments-form": {
				"css": "& input[type=submit] {width: auto;} & label {font-size: var(--wp--preset--font-size--x-small);}"
			},
			"core/post-template": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--medium);"
				}
			},
			"core/social-links": {
				"css": ".is-style-outline .wp-block-social-link {background-color: transparent;border: 1px solid currentColor;}",
				"spacing": {
					"blockGap": "10px"
				}
			},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "{overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": {
						"top": "25px",
						"right": "30px",
						"bottom": "25px",
						"left": "30px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"border": {
