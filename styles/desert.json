{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
    "title": "Desert Ridge",
	"description":  "Desert colors and box shadows",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "840px",
			"wideSize": "1200px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "Extra Small",
					"size": "8px",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "16px",
					"slug": "small"
				},
				{
					"name": "Medium ( 24px / 32px )",
					"size": "clamp(24px, calc(1.5rem + ((1vw - 7.68px) * 1.5625)), 32px)",
					"slug": "medium"
				},
				{
					"name": "Large ( 32px / 48px )",
					"size": "clamp(32px, calc(2rem + ((1vw - 7.68px) * 3.125)), 48px)",
					"slug": "large"
				},
				{
					"name": "Extra Large ( 32px / 64px )",
					"size": "clamp(32px, calc(2rem + ((1vw - 7.68px) * 6.25)), 64px)",
					"slug": "x-large"
				},
				{
					"name": "2xl ( 32px / 80px )",
					"size": "clamp(32px, calc(2rem + ((1vw - 7.68px) * 9.375)), 80px)",
					"slug": "xx-large"
				},
				{
					"name": "3xl ( 48px / 96px )",
					"size": "clamp(48px, calc(3rem + ((1vw - 7.68px) * 9.375)), 96px)",
					"slug": "xxx-large"
				}			
			],
			"units": [
				"px",
				"em",
				"rem",
				"svh",
				"vh",
				"vw",
				"%"
			]
		},
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [
				{
					"gradient": "linear-gradient(70deg, var(--wp--custom--color--primary--dark) 0%, var(--wp--preset--color--primary) 100%)",
					"name": "Primary to Primary Dark",
					"slug": "primary-primaryDark"
				},
				{
					"gradient": "linear-gradient(70deg, var(--wp--custom--color--secondary--dark) 0%, var(--wp--preset--color--secondary) 100%)",
					"name": "Secondary to Secondary Dark",
					"slug": "secondary-secondaryDark"
				},
				{
					"gradient": "linear-gradient(70deg, var(--wp--custom--color--alternate--dark) 0%, var(--wp--preset--color--alternate) 100%)",
					"name": "Alternate to Alternate Dark",
					"slug": "alternate-alternateDark"
				},
				{
					"gradient": "linear-gradient(70deg, var(--wp--preset--color--neutral-light) 0%, var(--wp--preset--color--neutral) 100%)",
					"name": "Neutral to Neutral Light",
					"slug": "neutral-neutralLight"
				},
				{
					"gradient": "linear-gradient(70deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--secondary) 100%)",
					"name": "Primary to Secondary",
					"slug": "primary-secondary"
				}
			],
			"duotone": [
				{
					"colors": [ "#595f23", "#ffffff" ],
					"slug": "primary",
					"name": "Primary"
				},
				{
					"colors": [ "#af844b", "#ffffff" ],
					"slug": "secondary",
					"name": "Secondary"
				},
				{
					"colors": [ "#a62a1c", "#ffffff" ],
					"slug": "alternate",
					"name": "Alternate"
				},
				{
					"colors": [ "#919A3B", "#ffffff" ],
					"slug": "highlight",
					"name": "Highlight"
				},
				{
					"colors": [ "#49535d", "#ffffff" ],
					"slug": "neutral-dark",
					"name": "Neutral Dark"
				}
			],
			"palette": [
				{
					"color": "#595f23",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#af844b",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#a62a1c",
					"name": "Alternate",
					"slug": "alternate"
				},
				{
					"color": "#919A3B",
					"name": "Highlight",
					"slug": "highlight"
				},
				{
					"color": "#FFDD38",
					"name": "Warning",
					"slug": "warning"
				},
				{
					"color": "#18A957",
					"name": "Success",
					"slug": "success"
				},
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#f5f0e9",
					"name": "Neutral Light",
					"slug": "neutral-light"
				},
				{
					"color": "#dadcde",
					"name": "Neutral",
					"slug": "neutral"
				},
				{
					"color": "#49535d",
					"name": "Neutral Dark",
					"slug": "neutral-dark"
				},
				{
					"color": "#0e161e",
					"name": "Contrast",
					"slug": "contrast"
				}
			]
			
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "'Playfair Display', serif",
					"name": "Brand",
					"slug": "brand",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "'Playfair Display'",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "500 700",
							"src": [ "file:./assets/fonts/Playfair_Display/PlayfairDisplay-VariableFont_wght.ttf" ]
						}
					]
				},
				{
					"fontFamily": "'Lato', sans-serif",
					"name": "Display",
					"slug": "display",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "'Lato'",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [ "file:./assets/fonts/Lato/Lato-Light.ttf"]
						}
					]
				},
				{
					"fontFamily": "Tahoma, Verdana, Segoe, sans-serif",
					"name": "Body",
					"slug": "body"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "xSmall",
					"size": "16px",
					"slug": "x-small"
				},
				{
					"fluid": false,
					"name": "Small",
					"size": "18px",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "18px",
						"max": "20px"
					},
					"name": "Medium",
					"size": "20px",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "20px",
						"max": "24px"
					},
					"name": "Large",
					"size": "24px",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "24px",
						"max": "30px"
					},
					"name": "xLarge",
					"size": "30px",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "30px",
						"max": "36px"
					},
					"name": "36px",
					"size": "36px",
					"slug": "max-36"
				},
				{
					"fluid": {
						"min": "36px",
						"max": "48px"
					},
					"name": "48px",
					"size": "48px",
					"slug": "max-48"
				},
				{
					"fluid": {
						"min": "42px",
						"max": "60px"
					},
					"name": "60px",
					"size": "60px",
					"slug": "max-60"
				},
				{
					"fluid": {
						"min": "48px",
						"max": "72px"
					},
					"name": "72px",
					"size": "72px",
					"slug": "max-72"
				}
			]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Light",
					"slug": "light",
					"shadow": "0 5px 10px -3px rgb(0 0 0 / 30%);"
				},
				{
					"name": "Dark",
					"slug": "dark",
					"shadow": "0 2px 5px -1px rgb(0 0 0 / 75%);"
				},
				{
					"name": "Diffused",
					"slug": "diffused",
					"shadow": "0 10px 20px -7px rgba(0,0,0,.3)"	
				}
			]
		},
		"custom": {	
			"linkColor": "var(--wp--preset--color--primary)",
			"linkHoverColor": "var(--wp--preset--color--highlight)",
			"textShadow": "0 .2rem .4rem rgba(0,0,0, 0.5)",
			"borderRadius": "0px",
			"buttonBorderRadius": "200px",
			"buttonShadow": "var(--wp--preset--shadow--light)", 
			"buttonHoverShadow": "var(--wp--preset--shadow--dark)",
			"boxShadow": "var(--wp--preset--shadow--light)",
			"boxHoverShadow": "var(--wp--preset--shadow--dark)",
			"transitionTime": "0.35s ease-in-out",	
			"color":{
				"primary": {
					"light" : "#acaf91",
					"dark": "#2d3012"
				},
				"secondary": {
					"light" : "#e7dac9",
					"dark": "#694f2d"
				},
				"alternate": {
					"light" : "#d3958e",
					"dark": "#53150e"
				},
				"deco": "var(--wp--preset--color--secondary)",
				"alert": {
					"success": "#18A957",
					"warning": "#FFDD38",
					"error": "#DF1642",
					"focus": "blue"
				}
			},
			"spacing": {
				"gap": "var(--wp--preset--spacing--large)"
			},
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"body": 1.75,
				"heading": 1.1,
				"medium": 1.5,
				"one": 1
			}

		},
		"blocks": {
			"core/button": {
				"spacing": {
					"spacingSizes": [
						{
							"name": "xSmall",
							"size": "2px",
							"slug": "x-small"
						},
						{
							"name": "Small",
							"size": "5px",
							"slug": "small"
						},
						{
							"name": "Medium",
							"size": "10px",
							"slug": "medium"
						},
						{
							"name": "Large",
							"size": "16px",
							"slug": "large"
						},
						{
							"name": "xLarge",
							"size": "24px",
							"slug": "x-large"
						},
						{
							"name": "Mega",
							"size": "36px",
							"slug": "mega"
						}
					]
				}
			}
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--gap)",
			"padding": {
				"top": "0px",
				"right": "var(--wp--custom--spacing--gap)",
				"bottom": "0px",
				"left": "var(--wp--custom--spacing--gap)"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		},
		"elements": {
			"button": {
				"shadow": "var(--wp--custom--button-shadow)",
				"border": {
					"radius": "var(--wp--custom--button-border-radius)"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"textTransform": "uppercase"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--small)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				},
				":hover": {
					"shadow": "var(--wp--custom--button-hover-shadow)",
					"typography": {
						"textDecoration": "none"
					},
					"color": {
						"background": "var(--wp--preset--color--highlight)"
					}
				},
				":focus": {
					"outline": {
						"color": "var(--wp--custom--color--alert--focus)"
					}
				}
			},
			"caption": {
				"spacing": {
					"margin": {
						"top": "10px",
						"bottom": "0px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--display)"
				}
			},
			"cite": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontStyle": "inherit"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--brand)",
					"fontSize": "var(--wp--preset--font-size--max-48)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--brand)",
					"fontSize": "var(--wp--preset--font-size--max-36)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				},
				"css": "a {color: var(--wp--preset--color--contrast);} a:hover {color: var(--wp--preset--color--primary);}"
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--semibold)"	
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--highlight)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--highlight)"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--focus)"
					},
					"outline": {
						"color": "var(--wp--custom--color--alert--focus)"
					}
				}
			}
		},
		"blocks": {
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0"
					}
				}
			},
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--base)"
				},
				"border": {
					"radius": "var(--wp--custom--button-border-radius)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--large)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--large)"
					}
				},
				"variations": {
					"outline" : {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						"shadow": "none",
						"border": {
							"color": "currentColor",
							"style": "solid",
							"width": "1px"
						},
						"spacing": {
							"padding": {
								"top": "calc( var(--wp--preset--spacing--medium) - 1px )",
								"right": "calc( var(--wp--preset--spacing--large) - 1px )",
								"bottom": "calc( var(--wp--preset--spacing--medium) - 1px )",
								"left": "calc( var(--wp--preset--spacing--large) - 1px )"
							}
						},
						"css": ":hover {color: var(--wp--preset--color--highlight);}"
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--small)"
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--gap)",
						"right": "var(--wp--custom--spacing--gap)",
						"bottom": "var(--wp--custom--spacing--gap)",
						"left": "var(--wp--custom--spacing--gap)"
					}
				}
			},
			"core/group": {
				"css": ""
			},
			"core/image": {
				"css": "",
				"spacing": {
					"margin": {
						"top": "0",
						"right": "0",
						"bottom": "0",
						"left": "0"
					}
				},
				"border": {
					"radius": "var(--wp--custom--border-radius)"
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "var(--wp--custom--spacing--gap)"
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--display)",
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--highlight)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--highlight)"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--highlight)"
							}
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--display)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/paragraph": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--highlight)"
							}
						}
					}
				},
				"css": ".has-background {padding: var(--wp--preset--spacing--medium) var(--wp--preset--spacing--large);}"
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"css": "& code {overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--gap)",
						"right": "var(--wp--custom--spacing--gap)",
						"bottom": "var(--wp--custom--spacing--gap)",
						"left": "var(--wp--custom--spacing--gap)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"border": {
					"radius": "var(--wp--custom--border-radius)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
